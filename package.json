  "scripts": {
    "check": "node ./node_modules/hexo/bin/hexo g && gulp html",
    "check:drafts": "node ./node_modules/hexo/bin/hexo g --drafts && gulp html",
    "repubprod": "rm cache4prodpublishing && hexo clean && hexo g && gulp build && gulp",
