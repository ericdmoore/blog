    "pub": "hexo clean && hexo g && gulp build && gulp",
    "webpack": "webpack",
    "publish:drafts": "npm run gen:drafts && gulp build && npm run publish",
    "publish": "gulp",
    "prod": "npm run clean && npm run gen && npm run build && npm run production",
    "production": "gulp prod",
    "clean": "hexo clean",
    "clear": "hexo clean",
    "gen": "hexo g",
    "gen:drafts": "hexo clean && hexo g --drafts",
    "build": "gulp build",
    "start": "hexo server -lo",
    "server": "hexo server -lo --drafts",
    "develop": "hexo server -lo --drafts",
    "server:static": "hexo server -slo --drafts",
    "zip": "zip -r function.zip *.js package.json node_modules",
    "lint": "eslint src"
  },
